<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FilterView">
        <attr name="entries" format="reference"/>
        <attr name="title" format="string"/>
        <attr name="elementPadding" format="dimension"/>
        <attr name="single" format="boolean"/>
        <attr name="mode" format="enum">
            <enum name="fixed" value="0"/>
            <enum name="wrap" value="1"/>
        </attr>
        <attr name="eleWidth" format="dimension"/>
        <attr name="eleHeight" format="dimension"/>
        <attr name="checkFirst" format="boolean"/>
    </declare-styleable>
</resources>